{
  "name": "inventory-backend",
  "version": "1.0.0",
  "description": "階層型在庫管理システム - バックエンド",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate && npm run build",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "inventory",
    "management",
    "hierarchical"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.0",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.0",
    "@types/node": "^20.0.0",
    "@types/papaparse": "^5.5.0",
    "@types/pdfkit": "^0.17.3",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
